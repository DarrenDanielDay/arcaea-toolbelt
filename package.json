{
  "name": "arcaea-toolbelt",
  "version": "0.0.0",
  "type": "module",
  "targets": {
    "default": {
      "context": "browser",
      "scopeHoist": false,
      "source": [
        "src/index.html",
        "src/services/cross-site-script.ts",
        "src/services/cross-site-frame.html"
      ],
      "outputFormat": "esmodule"
    }
  },
  "browserslist": "Chrome 100",
  "scripts": {
    "start:parcel": "parcel --target default --host localhost",
    "start:sheetly": "sheetly watch --dir src --hmr parcel",
    "start": "run-p --print-label \"start:**\"",
    "purge": "node ./scripts/purge.js",
    "build:parcel": "parcel build --target default --public-url /arcaea-toolbelt --no-optimize --no-source-maps",
    "build:sheetly": "sheetly build --dir src",
    "minify": "node ./scripts/minify.js",
    "build": "run-s purge build:sheetly build:parcel minify"
  },
  "private": true,
  "devDependencies": {
    "@parcel/transformer-inline-string": "^2.8.3",
    "@parcel/transformer-typescript-tsc": "^2.8.3",
    "esbuild": "^0.17.18",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.8.3",
    "purgecss": "^5.0.0",
    "typed-query-selector": "^2.10.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@fluentui/web-components": "^2.5.14",
    "sheetly": "^0.3.0",
    "tslib": "^2.5.0"
  }
}
